{
  "name": "cesium-use",
  "private": false,
  "version": "0.1.2",
  "description": "Vue powered CesiumJS utility library",
  "author": "s3xysteak",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "cesium",
    "vue",
    "utils"
  ],
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/s3xysteak/cesium-use",
  "bugs": "https://github.com/s3xysteak/cesium-use/issues",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/cesium-use.umd.cjs",
  "module": "./dist/cesium-use.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/cesium-use.js",
    "require": "./dist/cesium-use.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "run-p type:check \"build:only {@}\" --",
    "release": "pnpm build && pnpm publish",
    "build:only": "vite build",
    "type:check": "vue-tsc --build --force",
    "docs:dev": "vitepress dev src",
    "docs:build": "vitepress build src",
    "docs:preview": "vitepress preview src"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "cesium": "^1.115.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.25",
    "@unocss/transformer-directives": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "jsdom": "^23.2.0",
    "npm-run-all2": "^6.1.2",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.1.5",
    "vite-plugin-cesium-build": "^0.2.4",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "1.0.0-rc.44",
    "vitest": "^1.3.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  }
}
